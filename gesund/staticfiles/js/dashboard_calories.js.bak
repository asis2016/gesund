{% if calories_list %}
        <script>
            const myCalories = new Chart(
                document.getElementById('myCalories'),
                {
                    type: 'line',
                    data: {
                        labels: [{% for i in calories_list %}'{{ i.datestamp|date:"Y-m-d" }}', {% endfor %}],
                        datasets: [
                            {
                                data: [{% for i in calories_list %}{{ i.total_calories }}, {% endfor %}],
                                //data: [8000, 250, 7200]
                                label: '',
                                backgroundColor: '#dd7e77',
                                pointRadius: 6,
                            }
                        ]
                    },
                    options: {
                        responsive: true,
                        tooltips: {
                            enabled: false,
                            display: false,
                        },
                        plugins: {
                            legend: {
                                display: false,
                            },
                            title: {
                                display: false,
                            }
                        },
                        scales: {
                            x: {
                                ticks: {
                                    display: false
                                }
                            }
                        },
                    }
                }
            );
        </script>
    {% endif %}